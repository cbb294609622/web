<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>ListContactServlet</servlet-name>
    <servlet-class>com.cbb.servlet.ListContactServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddContactServlet</servlet-name>
    <servlet-class>com.cbb.servlet.AddContactServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>QueryContactServlet</servlet-name>
    <servlet-class>com.cbb.servlet.QueryContactServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateContactServlet</servlet-name>
    <servlet-class>com.cbb.servlet.UpdateContactServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteContactServlet</servlet-name>
    <servlet-class>com.cbb.servlet.DeleteContactServlet</servlet-class>
  </servlet>





  <servlet-mapping>
    <servlet-name>ListContactServlet</servlet-name>
    <url-pattern>/servlet/ListContactServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddContactServlet</servlet-name>
    <url-pattern>/servlet/AddContactServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>QueryContactServlet</servlet-name>
    <url-pattern>/servlet/QueryContactServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateContactServlet</servlet-name>
    <url-pattern>/servlet/UpdateContactServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteContactServlet</servlet-name>
    <url-pattern>/servlet/DeleteContactServlet</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
