<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <servlet-name>ResponseDemo1</servlet-name>
    <servlet-class>com.cbb.web.servlet.ResponseDemo1</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ResponseDemo2</servlet-name>
    <servlet-class>com.cbb.web.servlet.ResponseDemo2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ResponsePrintWriteDemo3</servlet-name>
    <servlet-class>com.cbb.web.servlet.ResponsePrintWriteDemo3</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DownLoadServletDemo1</servlet-name>
    <servlet-class>com.cbb.web.servletimg.DownLoadServletDemo1</servlet-class>
  </servlet>




  <servlet-mapping>
    <servlet-name>ResponseDemo1</servlet-name>
    <url-pattern>/servlet/ResponseDemo1</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResponseDemo2</servlet-name>
    <url-pattern>/servlet/ResponseDemo2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ResponsePrintWriteDemo3</servlet-name>
    <url-pattern>/servlet/ResponsePrintWriteDemo3</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DownLoadServletDemo1</servlet-name>
    <url-pattern>/servlet/DownLoadServletDemo1</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
